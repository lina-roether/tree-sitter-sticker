{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "sticker",
      "camelcase": "Sticker",
      "title": "Sticker",
      "scope": "source.sticker",
      "file-types": [
        "sticker"
      ],
      "injection-regex": "^sticker$",
      "class-name": "TreeSitterSticker",
      "highlights": "queries/highlights.scm",
      "tags": "queries/tags.scm"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "A simple stack-based toy programming language",
    "links": {
      "repository": "https://github.com/boxbeam/sticker"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false 
  }
}
