{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "sticker",
  "rules": {
    "source_file": {
      "type": "SEQ",
      "members": [
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "SYMBOL",
                    "name": "_sep"
                  },
                  {
                    "type": "BLANK"
                  }
                ]
              },
              {
                "type": "SYMBOL",
                "name": "function_def"
              }
            ]
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_sep"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "_sep": {
      "type": "PATTERN",
      "value": "\\p{White_Space}+"
    },
    "_char": {
      "type": "PATTERN",
      "value": "\\\\.|[^\"]"
    },
    "lit_int": {
      "type": "PATTERN",
      "value": "-?[0-9]+"
    },
    "lit_str": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\""
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_char"
          }
        },
        {
          "type": "STRING",
          "value": "\""
        }
      ]
    },
    "_lit": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "lit_int"
        },
        {
          "type": "SYMBOL",
          "name": "lit_str"
        }
      ]
    },
    "ident": {
      "type": "PATTERN",
      "value": "([[:alpha:]]|[+\\-*/$_=<>&%#~|.^])([[:alnum:]]|[+\\-*/$_=<>&%#~|.^])*"
    },
    "block": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_sep"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "_item"
                },
                {
                  "type": "REPEAT",
                  "content": {
                    "type": "SEQ",
                    "members": [
                      {
                        "type": "SYMBOL",
                        "name": "_sep"
                      },
                      {
                        "type": "SYMBOL",
                        "name": "_item"
                      }
                    ]
                  }
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_sep"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "_item": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_lit"
        },
        {
          "type": "SYMBOL",
          "name": "function_call"
        },
        {
          "type": "SYMBOL",
          "name": "block"
        }
      ]
    },
    "function_call": {
      "type": "SYMBOL",
      "name": "ident"
    },
    "function_def": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "fn"
        },
        {
          "type": "SYMBOL",
          "name": "_sep"
        },
        {
          "type": "SYMBOL",
          "name": "ident"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_sep"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "block"
        }
      ]
    }
  },
  "extras": [],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}